<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>scopeName</key>
		<string>text.log</string>

		<key>fileTypes</key>
		<array>
			<string>log</string>
		</array>

		<key>name</key>
		<string>Log file</string>

		<key>patterns</key>
		<array>
		
			<!-- DEBUG -->
			<dict>
				<key>match</key>
				<string>\b(DEBUG)\b|(?i)\b(debug)\:</string>
				<key>name</key>
				<string>markup.changed</string>
			</dict>

			<!-- DEBUG -->
			<dict>
				<key>match</key>
				<string>set\Kup</string>
				<key>name</key>
				<string>markup.changed</string>
			</dict>

			<!-- WARN -->
			<dict>
				<key>match</key>
				<string>\b(WARNING|WARN)\b|(?i)\b(warning)\:</string>
				<key>name</key>
				<string>markup.deleted</string>
			</dict>

			<!-- ERROR -->
			<dict>
				<key>match</key>
				<string>\b(ERROR|FAILURE|FAIL)\b|(?i)\b(error)\:</string>

				<key>name</key>
				<string>invalid</string>
			</dict>

			<!-- ARROW -->
			<dict>
				<key>match</key>
				<string><-|-></string>
				<key>name</key>
				<string>markup.bold</string>
			</dict>


			<!-- TIMESTAMP -->
			<dict>
				<key>match</key>
				<string>\[\d{8}\]</string>
				<key>name</key>
				<string>comment</string>
			</dict>
			
			<!-- SET STATE -->
			<dict>
				<key>match</key>
				<string>READER_\S+</string>
				<key>name</key>
				<string>constant.language</string>
			</dict>
		</array>
		
		<key>uuid</key>
		<string>E81BB6AB-CAC7-4C27-9A79-4137A4693EBD</string>
	</dict>
</plist>
