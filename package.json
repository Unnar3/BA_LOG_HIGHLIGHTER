{
	"name": "BANordicLogHighlighter",
	"displayName": "BA NORDIC log highlighter",
	"icon":"content/appicon.png",
	"description": "Adds color highlighting to log files to make it easier to follow the flow of log events and identify problems.",
	"categories": ["Other"],
	"version": "1.0.0",
	"bugs": {
		"url": "git@github.com:Unnar3/BA_LOG_HIGHLIGHTER.git/issues"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:Unnar3/BA_LOG_HIGHLIGHTER.git"
	},
	"engines": {
		"vscode": "^1.0.0"
	},
	"publisher": "unnar",
	"contributes": {
		"languages": [{
			"id": "log",
			"aliases": ["Log File", "log"],
			"extensions": [".log"]
		}],
		"grammars": [{
			"language": "log",
			"scopeName": "text.log",
			"path": "./syntaxes/log.tmLanguage"
		}]
	}
}